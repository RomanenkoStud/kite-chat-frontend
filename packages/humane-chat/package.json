{
  "name": "@pragmasoft-ukraine/humane-chat",
  "description": "humane-chat frontend",
  "license": "LGPL3.0",
  "author": "Dmytro V. Zemnytskyi <pragmasoft@gmail.com>",
  "version": "2022.9.5",
  "homepage": "https://pragmasoft-ua.github.io/humane-chat-frontend/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pragmasoft-ua/humane-chat-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/pragmasoft-ua/humane-chat-frontend/issues"
  },
  "keywords": [
    "humane",
    "chat",
    "webchat",
    "livechat",
    "support",
    "Ukraine",
    "web-components"
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "source": "src/index.ts",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "./src/humane-*",
    "./src/index.ts"
  ],
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "targets": {
    "module": {
      "includeNodeModules": true
    }
  },
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "clean": "rimraf dist",
    "test": "jest"
  },
  "dependencies": {
    "@msgpack/msgpack": "^2.8.0",
    "@pragmasoft-ukraine/humane-chat-component": "workspace:*",
    "@swc/helpers": "^0.4.11"
  }
}

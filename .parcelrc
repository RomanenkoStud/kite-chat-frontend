{
  "extends": "@parcel/config-default",
  "transformers": {
    "blob-url:*": [
      "@parcel/transformer-inline",
      "..."
    ],
    "*.css": [
      "@parcel/transformer-postcss",
      "@parcel/transformer-css",
      "@parcel/transformer-inline-string"
    ]
  },
  "optimizers": {
    "blob-url:*": [
      "...",
      "@parcel/optimizer-blob-url"
    ]
  }
}
